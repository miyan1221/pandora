{
    "name": "pandora-cloud",
    "version": 2,
    "framework": null,
    "builds": [
      {
        "src": "setup.py",
        "use": "@vercel/python"
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "setup.py"
      }
    ],
    "env": {
      "PANDORA_SERVER_LISTEN": "false"
    }
  }